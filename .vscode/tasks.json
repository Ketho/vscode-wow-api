// See https://go.microsoft.com/fwlink/?LinkId=733558
// for the documentation about the tasks.json format
{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"script": "watch",
			"problemMatcher": "$tsc-watch",
			"isBackground": true,
			"presentation": {
				"reveal": "never"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "Lua 5.4 wow-api",
			"type": "shell",
			"command": "lua54 LuaScripts/main.lua",
			"presentation": {
				"showReuseMessage": false,
				"clear": true
			},
			"problemMatcher": []
		},
		{
			"label": "Lua 5.4 parseWiki",
			"type": "shell",
			"command": "lua54 LuaScripts/parseWiki.lua",
			"presentation": {
				"showReuseMessage": false,
				"clear": true
			},
			"problemMatcher": []
		}
	]
}
